# Vibe Kanban Integration
# These environment variables configure the connection to the vibe-kanban service

# Backend API URL (default: auto-detect based on platform)
VIBE_KANBAN_API_URL=http://localhost:3001

# WebSocket URL for real-time updates
VIBE_KANBAN_WS_URL=ws://localhost:3001

# Frontend URL for embedding
VIBE_KANBAN_FRONTEND_URL=http://localhost:3000

# Individual port overrides (used if full URLs not specified)
VIBE_KANBAN_HOST=localhost
VIBE_KANBAN_BACKEND_PORT=3001
VIBE_KANBAN_FRONTEND_PORT=3000
