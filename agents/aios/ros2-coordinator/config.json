{
  "name": "ros2-coordinator",
  "description": "ROS2 robot coordination agent for natural language robot control",
  "version": "0.1.0",
  "author": "FlexNetOS",
  "license": "MIT",
  "entry": "entry.py",
  "aios_version": ">=0.0.3",
  "capabilities": [
    "robot-control",
    "natural-language-processing",
    "state-management"
  ],
  "tools": [
    "llm_chat",
    "memory_read",
    "memory_write"
  ],
  "environment": {
    "AGIXT_URL": "http://localhost:7437",
    "LOCALAI_URL": "http://localhost:8080"
  },
  "parameters": {
    "default_model": "localai/ggml-gpt4all-j",
    "temperature": 0.3,
    "max_tokens": 500
  }
}
